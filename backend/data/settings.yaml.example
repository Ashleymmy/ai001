# 配置文件示例
# 复制此文件为 settings.yaml 并填入你的 API Key

llm:
  provider: "doubao"  # 可选: openai, doubao, qwen, custom
  api_key: "YOUR_API_KEY_HERE"
  base_url: "https://ark.cn-beijing.volces.com/api/v3"
  model: "doubao-seed-1-6-251015"

image:
  provider: "custom_xxxxx"
  api_key: "YOUR_API_KEY_HERE"
  base_url: "https://ark.cn-beijing.volces.com/api/v3"
  model: "your-model-endpoint"

storyboard_image:
  provider: "custom_xxxxx"
  api_key: "YOUR_API_KEY_HERE"
  base_url: "https://dashscope.aliyuncs.com/api/v1"
  model: "wan2.6-i2v"

video:
  provider: "custom_xxxxx"
  api_key: "YOUR_API_KEY_HERE"
  base_url: "https://ark.cn-beijing.volces.com/api/v3"
  model: "your-video-endpoint"
